<?xml version="1.0" encoding="UTF-8"?>
<DescribeRegionsResponse xmlns="http://ec2.amazonaws.com/doc/2016-11-15/">
  <requestId>mock-request-id</requestId>
  <regionInfo>
{{- range .Regions}}
    <item>
      <regionName>{{.}}</regionName>
